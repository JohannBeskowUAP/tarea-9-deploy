generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Review {
  id        String   @id @default(cuid())
  bookId    String
  user      String
  text      String
  rating    Int
  votes     Int      @default(0)
  createdAt DateTime @default(now())
}
